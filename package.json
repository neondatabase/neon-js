{
  "name": "@neondatabase/neon-js",
  "version": "0.0.0",
  "description": "A TypeScript SDK for Neon services.",
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://github.com/neondatabase-labs/neon-js/",
  "bugs": {
    "url": "https://github.com/neondatabase-labs/neon-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neondatabase-labs/neon-js.git"
  },
  "author": "Pedro Figueiredo <pedro.figueiredo@databricks.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "neon-js": "./dist/cli/index.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:node": "node --run test",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@types/node": "24.7.2",
    "bumpp": "10.1.0",
    "jsdom": "^27.0.1",
    "msw": "^2.6.8",
    "tsdown": "0.15.8",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.3"
  },
  "dependencies": {
    "@stackframe/js": "2.8.41",
    "@supabase/auth-js": "2.72.0",
    "@supabase/postgres-meta": "0.93.0",
    "@supabase/postgrest-js": "2.75.0",
    "meow": "^14.0.0",
    "zod": "4.1.12"
  }
}
