{
  "name": "@neondatabase/neon-js",
  "version": "0.1.0-alpha.11",
  "description": "A TypeScript SDK for Neon services",
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://github.com/neondatabase-labs/neon-js/",
  "bugs": {
    "url": "https://github.com/neondatabase-labs/neon-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neondatabase-labs/neon-js.git"
  },
  "author": "Pedro Figueiredo <pedro.figueiredo@databricks.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "neon-js": "dist/cli/index.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client/index.d.mts",
      "default": "./dist/client/index.mjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.mts",
      "default": "./dist/cli/index.mjs"
    },
    "./auth": {
      "types": "./dist/auth/index.d.mts",
      "default": "./dist/auth/index.mjs"
    },
    "./auth/react": {
      "types": "./dist/auth/react/index.d.mts",
      "default": "./dist/auth/react/index.mjs"
    },
    "./auth/react/ui": {
      "types": "./dist/auth/react/ui/index.d.mts",
      "default": "./dist/auth/react/ui/index.mjs"
    },
    "./auth/react/adapters": {
      "types": "./dist/auth/react/adapters/index.d.mts",
      "default": "./dist/auth/react/adapters/index.mjs"
    },
    "./auth/vanilla": {
      "types": "./dist/auth/vanilla/index.d.mts",
      "default": "./dist/auth/vanilla/index.mjs"
    },
    "./auth/vanilla/adapters": {
      "types": "./dist/auth/vanilla/adapters/index.d.mts",
      "default": "./dist/auth/vanilla/adapters/index.mjs"
    },
    "./auth/next": {
      "types": "./dist/auth/next/index.d.mts",
      "default": "./dist/auth/next/index.mjs"
    },
    "./auth/react/ui/css": {
      "types": "./dist/auth/react/ui/css.d.ts",
      "default": "./dist/auth/react/ui/css.css"
    },
    "./auth/react/ui/tailwind": {
      "default": "./dist/auth/react/ui/tailwind.css"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:node": "node --run test",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit",
    "test:types": "tsc --noEmit",
    "release": "bun run build && bumpp --tag 'neon-js-v%s' && bun publish --tag latest",
    "lint": "eslint .",
    "clean": "rm -rf node_modules dist"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "tsdown": "0.16.0",
    "typescript": "^5.9.3",
    "vitest": "4.0.7",
    "vite-tsconfig-paths": "5.1.4",
    "msw": "2.6.8"
  },
  "dependencies": {
    "@neondatabase/postgrest-js": "workspace:*",
    "@neondatabase/auth": "workspace:*",
    "@supabase/postgres-meta": "0.93.1",
    "meow": "14.0.0",
    "zod": "4.1.12"
  }
}
