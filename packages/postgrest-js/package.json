{
  "name": "@neondatabase/postgrest-js",
  "version": "0.1.0-alpha.2",
  "description": "PostgreSQL client for Neon Data API - query your database without authentication",
  "type": "module",
  "license": "Apache-2.0",
  "keywords": [
    "neon",
    "neondb",
    "postgresql",
    "postgres",
    "database",
    "postgrest",
    "data-api",
    "typescript",
    "sdk",
    "query-builder"
  ],
  "homepage": "https://github.com/neondatabase/neon-js/tree/main/packages/postgrest-js#readme",
  "bugs": {
    "url": "https://github.com/neondatabase/neon-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neondatabase/neon-js.git",
    "directory": "packages/postgrest-js"
  },
  "funding": {
    "url": "https://neon.tech"
  },
  "author": "Pedro Figueiredo <pedro.figueiredo@databricks.com>",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:node": "node --run test",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit",
    "test:types": "tsc --noEmit",
    "release": "bun run build && bumpp --tag 'postgrest-js-v%s' && bun publish --tag latest",
    "lint": "eslint .",
    "clean": "rm -rf node_modules dist"
  },
  "devDependencies": {
    "@neondatabase/internal": "workspace:*"
  },
  "dependencies": {
    "@supabase/postgrest-js": "2.79.0"
  }
}
