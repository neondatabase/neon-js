{
  "name": "@neondatabase/postgrest-js",
  "version": "0.1.0-alpha.1",
  "description": "Generic PostgreSQL client for Neon Data API (without authentication)",
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://github.com/neondatabase-labs/neon-js/",
  "bugs": {
    "url": "https://github.com/neondatabase-labs/neon-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neondatabase-labs/neon-js.git"
  },
  "author": "Pedro Figueiredo <pedro.figueiredo@databricks.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.mjs",
    "./client": "./dist/client/index.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:node": "node --run test",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit",
    "test:types": "tsc --noEmit",
    "release": "bun run build && bumpp --tag 'postgrest-js-v%s' && bun publish --tag latest",
    "lint": "eslint .",
    "clean": "rm -rf node_modules dist"
  },
  "devDependencies": {},
  "dependencies": {
    "@supabase/postgrest-js": "2.79.0"
  }
}
