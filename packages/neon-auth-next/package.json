{
  "name": "@neondatabase/neon-auth-next",
  "version": "0.1.0-alpha.24",
  "description": "Neon Auth Client for Next.js",
  "type": "module",
  "license": "Apache-2.0",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:node": "node --run test",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit",
    "test:types": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf node_modules dist",
    "release": "bun run build && bumpp --tag 'neon-auth-next-v%s' && bun publish --tag latest"
  },
  "devDependencies": {
    "tsdown": "0.16.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "next": "^16.0.6"
  },
  "dependencies": {
    "@neondatabase/neon-auth": "workspace:*"
  }
}
